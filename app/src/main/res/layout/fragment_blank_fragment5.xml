<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BlankFragment5">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="@drawable/creative" />
        <android.support.v7.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            android:id="@+id/cv691"
            android:elevation="5dp"
            android:background="@drawable/textviewstyle">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/basic"
        android:text="creative entrepreneurs"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textSize="22sp"
        android:layout_margin="10dp"
        android:textStyle="bold"
        android:background="@drawable/textviewstyle"/>
        </android.support.v7.widget.CardView>


    <LinearLayout
        android:id="@+id/ll1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <android.support.v7.widget.CardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:cardCornerRadius="10dp"
        android:layout_margin="10dp"
        android:id="@+id/cv621"
        android:elevation="5dp"
        android:background="@drawable/textviewstyle">

        <LinearLayout
            android:id="@+id/ll622"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1.0"
                android:scaleType="centerCrop"
                android:layout_gravity="center"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/creative2"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/basic"
                android:text="Entrepreneurship is not and never restricted to any specific field. Any person or individual who comes with a new innovative idea in their respective field can be treated as an Entrepreneur. E-Summit'19 comes up with a special event called Creative Entrepreneurs in which entrepreneurs from diversified sectors such as Art,Music etc will be sharing their experiences "
                android:layout_margin="10dp"
                android:textSize="16sp"
                />
        </LinearLayout>

    </android.support.v7.widget.CardView>
    </LinearLayout>
    </LinearLayout>
    </ScrollView>
    </LinearLayout>
</FrameLayout>

